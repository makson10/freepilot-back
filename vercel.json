{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "yarn run build",
  "outputDirectory": "dist",
  "devCommand": "yarn run start:dev",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}
